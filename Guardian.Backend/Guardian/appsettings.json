    "DisplayName": "Test test",
    "Password": "YourGmailPassword",
    "Host": "smtp.gmail.com",
    "Port": 587
  },
  "JWTSettings": {
    "Key": "1105D15CB0D48F5781C103A18D5599E4FF25C9102FA694ABDF1DA6828BF153DE",
    "Issuer": "Identity",
