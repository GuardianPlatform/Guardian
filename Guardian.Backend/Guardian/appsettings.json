{
  "ApplicationDetail": {
    "ApplicationName": "Guardian",
    "Description": "Digital Distribution",
    "ContactWebsite": "https://github.com/GuardianPlatform/Guardian",
    "LicenseDetail": "https://opensource.org/licenses/MIT"
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "D:\\Logs\\log-{Date}.log",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level}] {Message}{NewLine}{Exception}"
        }
      },
      {
        "Name": "MSSqlServer",
        "Args": {
          "connectionString": "Data Source=(localdb)\\MSSQLLocalDB;Initial Catalog=OnionDb;Integrated Security=True",
          "sinkOptionsSection": {
            "tableName": "Logs",
            "schemaName": "EventLogging",
            "autoCreateSqlTable": true
          },
          "restrictedToMinimumLevel": "Warning"
        }
      }
    ],
    "Properties": {
      "Application": "Guardian - Digital Distribution"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "OnionArchConn": "Data Source=(localdb)\\MSSQLLocalDB;Initial Catalog=GuardianDb;Integrated Security=True",
    "IdentityConnection": "Data Source=(localdb)\\MSSQLLocalDB;Initial Catalog=GuardianDb;Integrated Security=True"
  },
  "FeatureManagement": {
    "EnableEmailService": false
  },
  "MailSettings": {
